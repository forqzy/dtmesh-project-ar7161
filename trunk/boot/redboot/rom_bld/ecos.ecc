    # Requires:  (CYG_HAL_STARTUP == "ROM") || (CYG_HAL_STARTUP == "ROMRAM") 
    #     CYG_HAL_STARTUP == ROMRAM
    #     CYG_HAL_STARTUP == ROMRAM
